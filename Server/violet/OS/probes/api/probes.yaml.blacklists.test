application=webui

[add]
url=blacklists/add
post_data=@yaml/blacklists.yml
content_type=text/yaml
regex=~

[get]
url=blacklists/get
post_data=@yaml/blacklists.yml
content_type=text/yaml
regex=PersonInformation

[remove]
url=blacklists/remove
post_data=@yaml/blacklists.yml
content_type=text/yaml
regex=~

[get_after]
url=blacklists/get
post_data=@yaml/blacklists.yml
content_type=text/yaml
regex=\[\]