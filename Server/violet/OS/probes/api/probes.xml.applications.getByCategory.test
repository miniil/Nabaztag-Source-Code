application=webui

[entertainement]
url=applications/getByCategory
post_data=<map><id>"LOC_srv_category_entertainement/title"</id><hw_type>"violet.nabaztag.tag"</hw_type></map>
regex=violet.nabaztag.tag

[daylife]
url=applications/getByCategory
post_data=<map><id>"LOC_srv_category_daylife/title"</id><hw_type>"violet.nabaztag.tag"</hw_type></map>
