;PauseTime
;HKR,Ndi\Params\PauseTime,  ParamDesc,  0, "PauseTime"
;HKR,Ndi\Params\PauseTime,  Type,       0, "int"
;HKR,Ndi\Params\PauseTime,  Default,    0, "-1"
